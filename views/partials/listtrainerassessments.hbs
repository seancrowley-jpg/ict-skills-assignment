<table class="ui celled table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Weight</th>
      <th>Chest</th>
      <th>Thigh</th>
      <th>Upperarm</th>
      <th>Waist</th>
      <th>Hips</th>
      <th>Trend</th>
      <th>Comment</th>
      <th>Edit Comment</th>
    </tr>
  </thead>
  <tbody>
    {{#each member.assessments}}
      <tr>
        <td>
          {{date}}
        </td>
        <td>
          {{weight}}
        </td>
        <td>
          {{chest}}
        </td>
        <td>
          {{thigh}}
        </td>
        <td>
          {{upperarm}}
        </td>
        <td>
          {{waist}}
        </td>
        <td>
          {{hips}}
        </td>
        <td>
          {{trend}}
        </td>
        <td>
          {{comment}}
        </td>
        <td>
          <a href="/comment/{{userid}}/editcomment/{{assessmentid}}" class="ui icon button">
            <i class="pencil alternate icon"></i>
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>