<table class="ui celled table">
  <thead>
    <tr>
      <th>Weight</th>
      <th>Chest</th>
      <th>Thigh</th>
      <th>Upperarm</th>
      <th>Waist</th>
      <th>Hips</th>
    </tr>
  </thead>
  <tbody>
    {{#each assessment}}
      <tr>
        <td>
          {{weight}}
        </td>
        <td>
          {{chest}}
        </td>
        <td>
          {{thigh}}
        </td>
        <td>
          {{upperarm}}
        </td>
        <td>
          {{waist}}
        </td>
        <td>
          {{hips}}
        </td>
        <td>
          <a href="/dashboard/deleteassessment/{{assessment.id}}" class="ui icon button"> <i class="icon trash"></i> </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>